public class ApplicationString {
	// must have a main method
	// type in main press Ctrl+Space hit Return
	public static void main(String[] args) {
		int myInt = 7;
		
		String text = "Hello";
		
		String blank = "  ";
		
		String name = "Bob";
		
		String greeting = text + blank + name;
		
