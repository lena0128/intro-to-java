public class Application {
	public static void main(String[] args) {
		
		int[] values = {3, 4, 2543};
		
		// System.out.println(values[2]);
		
		int[][] grid = {
				
				{3, 4, 2543}, // first row, don't forget you start index at 0
				{2, 4},  // second row
				{1, 2, 3, 4}   // third row
		};
		
		// System.out.println(grid[1][1]); // row 1 column 1
		// System.out.println(grid[0][2]); // row 0 column 2
		
		
		// iterating over the two-dimensitional array
		// first iterate through the rows
		for (int row = 0; row < grid.length; row++) {
			for (int col = 0; col < grid[row].length; col++) {
				System.out.println(grid[row][col] + "\t");
			}
		}
		
		String[][] texts = new String[2][3];
		
		texts[0][1] = "Hello there";
		
		// System.out.println(texts[0][1]);
	}
	
